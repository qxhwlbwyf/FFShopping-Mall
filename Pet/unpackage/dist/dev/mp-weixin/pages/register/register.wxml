<view><view class="img flex  padding align-center justify-center "><view class="bg-grey padding-sm margin-xs cu-avatar xl round margin-left" style="background-image:url(https://ss1.bdstatic.com/70cFvXSh_Q1YnxGkpoWK1HF6hhy/it/u=2649574139,2472850608&fm=26&gp=0.jpg);"></view></view><form><view class="cu-form-group margin-top"><view class="title">生日</view><view class="uni-list-cell-db"><picker mode="date" value="{{date}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{date}}</view></picker></view><input disabled="true"/></view><view class="cu-form-group"><view class="title">邮箱</view><input placeholder="电子邮箱" name="input" data-event-opts="{{[['input',[['__set_model',['$0','email','$event',[]],['user']]]]]}}" value="{{user.email}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">性别</view><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" style="width:700rpx;" bindchange="__e"><block wx:for="{{items}}" wx:for-item="item" wx:for-index="index" wx:key="value"><label class="uni-list-cell uni-list-cell-pd" style="float:left;margin:0rpx 40rpx;"><view><radio value="{{item.value}}" checked="{{index===current}}"></radio></view><view>{{item.name}}</view></label></block></radio-group></view><view class="cu-form-group"><view class="title">用户名</view><input placeholder="用户名" name="input" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['user']]]]]}}" value="{{user.username}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">真实姓名</view><input placeholder="真实姓名" name="input" data-event-opts="{{[['input',[['__set_model',['$0','realname','$event',[]],['user']]]]]}}" value="{{user.realname}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">密码</view><input placeholder="密码" name="input" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['user']]]]]}}" value="{{user.password}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">手机号码</view><input placeholder="输入框带标签" name="input" data-event-opts="{{[['input',[['__set_model',['$0','mobilePhone','$event',[]],['user']]]]]}}" value="{{user.mobilePhone}}" bindinput="__e"/><view class="cu-capsule radius"><view class="cu-tag bg-blue ">+86</view><view class="cu-tag line-blue">中国大陆</view></view></view><view class="cu-form-group"><view class="title">证件类型</view><radio-group data-event-opts="{{[['change',[['cardchange',['$event']]]]]}}" style="width:700rpx;" bindchange="__e"><block wx:for="{{cards}}" wx:for-item="item" wx:for-index="index" wx:key="value"><label class="uni-list-cell uni-list-cell-pd" style="float:left;margin:0rpx 40rpx;"><view><radio value="{{item.value}}" checked="{{index===no}}"></radio></view><view>{{item.name}}</view></label></block></radio-group></view><view class="cu-form-group"><view class="title">证件号码</view><input placeholder="编号" name="input" data-event-opts="{{[['input',[['__set_model',['$0','identityCardNo','$event',[]],['user']]]]]}}" value="{{user.identityCardNo}}" bindinput="__e"/></view><view><view class="margin-tb-lg" style="margin-left:550rpx;margin-top:10rpx;"><button data-event-opts="{{[['tap',[['register',['$event']]]]]}}" class="cu-btn round line-blue" bindtap="__e">注册</button></view></view></form></view>